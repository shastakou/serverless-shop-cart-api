{
  "name": "serverless-shop-cart-api",
  "version": "0.0.1",
  "description": "",
  "author": "Ivan Shastakou",
  "private": true,
  "license": "MIT",
  "engines": {
    "npm": "^8.19.3",
    "node": "^16.19.1"
  },
  "scripts": {
    "prebuild": "rimraf dist && prisma generate",
    "build": "nest build",
    "build:dev": "nest build --watch",
    "postbuild": "cp node_modules/.prisma/client/schema.prisma dist && cp node_modules/.prisma/client/libquery_engine-rhel-openssl-1.0.x.so.node dist",
    "start": "nest start",
    "start:lambda": "sls offline start",
    "start:debug": "export SLS_DEBUG=* && node --inspect /usr/local/bin/serverless offline",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "eb-init": "eb init shastakou-cart-service --region eu-west-1",
    "eb-create": "eb create shastakou-cart-service-dev --single --cname shastakou-cart-service-dev",
    "eb-terminate": "eb terminate",
    "deploy:eb": "eb deploy",
    "deploy:lambda": "sls deploy --verbose"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.12",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.12",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@prisma/client": "^4.12.0",
    "@vendia/serverless-express": "^4.10.1",
    "bcryptjs": "^2.4.3",
    "helmet": "^6.0.0",
    "nestjs-prisma": "^0.20.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.3.0",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.3.12",
    "@serverless/typescript": "^3.29.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "28.1.2",
    "@types/node": "^18.15.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "esbuild": "^0.17.14",
    "eslint": "8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "28.1.2",
    "prettier": "2.7.1",
    "prisma": "^4.12.0",
    "rimraf": "^3.0.2",
    "serverless": "^3.29.0",
    "serverless-esbuild": "^1.43.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-scripts": "^1.0.2",
    "supertest": "6.2.4",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "5.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
